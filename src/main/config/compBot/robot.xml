<?xml version="1.0"?>
<!DOCTYPE robot SYSTEM "robot.dtd">
<robot>
   
    
<!-- ====================================================
     Pigeon
     ==================================================== -->    
    <pigeon canId="50" />

       <!-- LED -->
       <led canId="30"
            usage = "2" />
       <led canId="31"
            usage = "1" />
       <led canId="32"
            usage = "0" />
       

<!-- ====================================================
     PDP
     ==================================================== -->    
    <pdp canId="51" />
    
<!-- ====================================================
     Lidars
     ==================================================== -->    
    <!-- Lidar 1 -->
    <lidar usage = "0"
           inputpin = "5"
           triggerpin = "6"/>
    <!-- Lidar 2 -->
    <lidar usage = "1"
           inputpin = "7"
           triggerpin = "8"/>
<!-- ====================================================
     camera
    ==================================================== -->

<!-- ====================================================
     limelight servo  
     ==================================================== -->
<servo usage="0"
       pwmId="1"
       minAngle="0.0"
       maxAngle="360.0" />
       

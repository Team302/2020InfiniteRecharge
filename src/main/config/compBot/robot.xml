<!-- ====================================================
     Chassis
     ====================================================  -->
    <chassis wheelDiameter="6.0"
             wheelBase="25.0"  
             track="18.0">
             
        <!-- Left (middle, front, back) -->
         <motor usage="MIDDLE_LEFT_DRIVE"
               canId="1"
               type="sparkmax_brushless"
               inverted="true"
               feedbackDevice="0"
               countsPerRev="1"
               brakeMode="true"
               gearRatio="0.16666666" 
               peakCurrentDuration="50"
               continuousCurrentLimit="25"
               peakCurrentLimit="40" 
               currentLimiting="true"  />
        <motor usage="FRONT_LEFT_DRIVE"
               canId="16"
               pdpID="0"
               type="sparkmax_brushless"
               feedbackDevice="0"
               countsPerRev="1"
               brakeMode="true"
               slaveTo="1"
               gearRatio="0.16666666" 
               peakCurrentDuration="50"
               continuousCurrentLimit="25"
               peakCurrentLimit="40" 
               currentLimiting="true"  />

         <motor usage="BACK_LEFT_DRIVE"
               canId="3"
               type="sparkmax_brushless"
               inverted="true"
               feedbackDevice="0"
               countsPerRev="1"

               type="sparkmax_brushless"
               inverted="true"
               feedbackDevice="0"
               countsPerRev="1"
               brakeMode="true"
               slaveTo="14"
               gearRatio="0.16666666" 
               peakCurrentDuration="50"
               continuousCurrentLimit="25"
               peakCurrentLimit="40" 
               currentLimiting="true"  />
        <motor usage="BACK_RIGHT_DRIVE"
               canId="12"
               type="sparkmax_brushless"
               inverted="true"
               feedbackDevice="0"
               countsPerRev="1"
               brakeMode="true"
               slaveTo="14"
               gearRatio="0.16666666" 
               peakCurrentDuration="50"
               continuousCurrentLimit="25"
               peakCurrentLimit="40" 
               currentLimiting="true"  />
        <PID mode="1"
             proportional="0.5"/>
        <PID mode="0"
             proportional="0.0001"
             integral="0.00001"/>

               

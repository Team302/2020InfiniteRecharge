               slaveTo="1"
               gearRatio="0.16666666" 
               peakCurrentDuration="50"
               continuousCurrentLimit="25"
               peakCurrentLimit="40" 
               currentLimiting="true"  />       
               
        <!-- Right (middle, front, back) -->
        <motor usage="MIDDLE_RIGHT_DRIVE"
               canId="14"
               type="sparkmax_brushless"
               feedbackDevice="0"
               countsPerRev="1"
               brakeMode="true"
               gearRatio="0.16666666"
               peakCurrentDuration="50"
               continuousCurrentLimit="25"
               peakCurrentLimit="40" 
               currentLimiting="true"  />
        <motor usage="FRONT_RIGHT_DRIVE"
